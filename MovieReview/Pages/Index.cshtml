@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Filip Movie Review</h1>
    <p>Lets rate movies :)</p>
</div>

<div class="card-column">
    @foreach (var movie in Model.Movies)
    {

        <div class="card">
            <div class="card-body">
                <h5 class="card-title">@movie.Name</h5>
            </div>
        </div>
        
    }
</div>
